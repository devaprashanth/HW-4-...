
#include <stdio.h>

int main() {
    int n, i, j, sum;
    scanf("%d", &n);
    int arr[n];
    for(i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }
    for(i = 0; i < n; i++) {
        sum = 0;
        for(j = i+1; j < n; j++) {
            sum += arr[j];
        }
        printf("%d ", sum);
    }
    return 0;
}

