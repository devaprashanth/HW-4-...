#include <stdio.h>

int main() {
    int n, i, zc = 0, oc = 0;
    scanf("%d", &n);
    int arr[n];
    for(i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
        if(arr[i] == 0) {
            zc++;
        } else if(arr[i] == 1) {
            oc++;
        }
    }
    printf("zc = %d\noc = %d", zc, oc);
    return 0;
}
